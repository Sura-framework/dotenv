{
  "name": "vitorsreis/dotenv",
  "description": "PHP DotEnv - Simple library for load .env files in php runtime with static class",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/vitorsreis/dotenv",
  "authors": [
    {
      "name": "Vitor Reis",
      "role": "developer",
      "email": "vitor@d5w.com.br"
    }
  ],
  "autoload": {
    "files": [
      "src/bootstrap.php"
    ],
    "psr-4": {
      "DotEnv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotEnv\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "keywords": [
    "php-dotenv",
    "dotenv",
    ".env",
    "env",
    "php"
  ]
}
